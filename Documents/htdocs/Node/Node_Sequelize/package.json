{
	"name": "node_sequelize",
	"version": "1.0.0",
	"description": "A Node app using Sequelize and PostgreSql",
	"main": "index.js",
	"scripts": {
		"dev": "nodemon src/index.js --exec babel-node",
		"build": "babel src --out-dir dist",
		"start": "node dist/index.js",
		"test": "nyc mocha --no-timeout './tests' --exit",
		"test:report": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-summary mocha --timeout 5000 './tests' --exit",
		"coverage": "cross-env NODE_ENV=test nyc npm run test && nyc report --reporter=lcov --reporter=text-lcov --exit",
		"db:migrate": "./node_modules/.bin/sequelize db:migrate",
		"migrate": "./node_modules/.bin/sequelize db:migrate",
		"undo": "./node_modules/.bin/sequelize db:migrate:undo:all",
		"seed": "./node_modules/.bin/sequelize db:seed:all",
		"sequelize": "sequelize init"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Tueloper/Sequelize-app.git"
	},
	"keywords": [
		"Node",
		"Sequelize",
		"App",
		"Postgress"
	],
	"author": "Tueloper",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Tueloper/Sequelize-app/issues"
	},
	"homepage": "https://github.com/Tueloper/Sequelize-app#readme",
	"dependencies": {
		"@babel/polyfill": "^7.8.3",
		"body-parser": "^1.19.0",
		"cross-env": "^7.0.0",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"morgan": "^1.9.1",
		"pg": "^7.18.1",
		"pg-hstore": "^2.3.3",
		"sequelize": "^5.21.3",
		"sequelize-cli": "^5.5.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.8.4",
		"@babel/node": "^7.8.4",
		"@babel/preset-env": "^7.8.4",
		"@babel/register": "^7.8.3",
		"chai": "^4.2.0",
		"chai-http": "^4.3.0",
		"coveralls": "^3.0.9",
		"mocha": "^7.0.1",
		"nodemon": "^2.0.2",
		"nyc": "^15.0.0"
	},
	"nyc": {
		"exclude": [
			"src/models/index.js",
			"tests",
			"node_modules"
		],
		"require": [
			"@babel/register"
		],
		"sourceMap": false,
		"cache": false,
		"report-dir": "tests/report"
	}
}
